{
  "name": "capeandbay-devs/clicks-ahoy",
  "description": "ClickUp SaaS Personal Token API Interface for Laravel",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "capeandbay",
    "click-up",
    "personal-tokens"
  ],
  "authors": [
    {
      "name": "Angel Gonzalez",
      "email": "angel@capeandbay.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^7.4|^8.0",
    "ixudra/curl": "^6.21",
    "lorisleiva/laravel-actions": "^2.1",
    "illuminate/support": "^7.0|^8.0|^9.0"
  },
  "autoload": {
    "psr-4": {
      "CapeAndBay\\ClicksAhoy\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "CapeAndBay\\ClicksAhoy\\CnBClickUpServiceProvider"
      ],
      "aliases": {
        "ClickUp": "CapeAndBay\\ClicksAhoy\\Facades\\ClickUp"
      }
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
